  object QLista: TSQLQuery
    SQLConnection = DM.BancoDados
    Params = <
      item
        DataType = ftDate
        Name = 'DATAINI'
        ParamType = ptInput
      end
      item
        DataType = ftDate
        Name = 'DATAFIN'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'COD_FOR'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'A'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'COD_CLIENTE'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'B'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'COD_CONTATO'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'C'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'COD_MOTORISTA'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'D'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'COD_AGENCIA'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'E'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'NUM_PEDIDO'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'F'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'NUM_CARGA'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'G'
        ParamType = ptInput
      end>
    SQL.Strings = (
      'Select'
      '   FO.NOME AS NOME_FOR,'
      '   CLI.NOME AS NOME_CLIENTE,'
      '   CON.NOME AS NOME_CONTATO,'
      '   PED.TOTAL_LIQUIDO,'
      '   PED.COD_CLIENTE,'
      '   PED.DATA,'
      '   PED.TOTAL_VENDA,'
      '   PED.TOTAL_QTDE,'
      '   PED.SITUACAO AS PED_ENCERRADO,'
      '   PED.TOTAL_LIQUIDO / TOTAL_QTDE AS PRECO_CUSTO,'
      '   PED.TOTAL_VENDA / TOTAL_QTDE AS PRECO_VENDA,'
      '   PED.TOTAL_LUCRO / TOTAL_QTDE AS VALOR_LUCRO,'
      '   PED.NUM_PEDIDO,'
      '   CAR.NUM_CARGA,'
      '   CAR.VALOR_FRETE,'
      '   CAR.VALOR_CARREGA,'
      '   CAR.SITUACAO,'
      '   CAR.FINANCEIRO,'
      '   CAR.COMPLEMENTO,'
      '   CAR.ID_CARGA,'
      '   CAR.LETRA'
      'from PEDIDO PED'
      '   left join CARGA CAR on PED.NUM_PEDIDO = CAR.NUM_PEDIDO'
      '   left join FORNECEDOR FO on PED.COD_FOR = FO.COD_FOR'
      '   left join CLIENTE CLI on PED.COD_CLIENTE = CLI.COD_CLIENTE'
      '   left join CLIENTE CON on PED.COD_CONTATO = CON.COD_CLIENTE'
      '   where PED.DATA between :DATAINI and :DATAFIN'
      '   and ((PED.COD_FOR = :COD_FOR) or (:A = 0))'
      '   and ((PED.COD_CLIENTE = :COD_CLIENTE) or (:B = 0))'
      '   and ((PED.COD_CONTATO = :COD_CONTATO) or (:C = 0))'
      '   and ((CAR.COD_MOTORISTA = :COD_MOTORISTA) or (:D = 0))'
      '   and ((CAR.COD_AGENCIA = :COD_AGENCIA) or (:E = 0))'
      '   and ((CAR.NUM_PEDIDO = :NUM_PEDIDO) or (:F = 0))'
      '   and ((CAR.NUM_CARGA = :NUM_CARGA) or (:G = 0))'
      '   order by PED.NUM_PEDIDO desc')
    Left = 168
    Top = 288
  end
