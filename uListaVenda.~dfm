  object Qcontas: TSQLDataSet
    SQLConnection = DM.BancoDados
    CommandText = 
      'select '#13#10'BAN.NUM_CONTA,'#13#10'sum(CON.VALOR_PAGO) as VALORPAGO'#13#10'from ' +
      'CONTAS CON'#13#10' left join CONTABANCO BAN on CON.ID_CONTABANCO = BAN' +
      '.ID_CONTABANCO'#13#10' where CON.NUM_PEDIDO = :NUM_PEDIDO'#13#10' and CON.TI' +
      'PO_CONTA = 2'#13#10' group by BAN.NUM_CONTA'
    Params = <
      item
        DataType = ftInteger
        Name = 'NUM_PEDIDO'
        ParamType = ptInput
      end>
    Left = 336
    Top = 352
    object QcontasVALORPAGO: TFloatField
      FieldName = 'VALORPAGO'
    end
    object QcontasNUM_CONTA: TStringField
      FieldName = 'NUM_CONTA'
    end
  end
