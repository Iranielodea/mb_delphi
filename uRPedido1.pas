procedure TFrPedido1.PedidoCalcFields(DataSet: TDataSet);
begin
   PedidoC_COMISSAOUNITARIO.AsFloat:=0;
   PedidoC_TOTALCOMISSAO.AsFloat:=0;
   if PedidoCOMISSAO_PEDIDO.AsFloat > 0 then
   begin
      PedidoC_COMISSAOUNITARIO.AsFloat:=Rateio(PedidoTOTAL_COMPRA.AsFloat, PedidoCOMISSAO_PEDIDO.AsFloat,PedidoTOTAL_LIQUIDO.AsFloat);
      PedidoC_TOTALCOMISSAO.AsFloat:=PedidoQTDE.AsFloat * PedidoC_COMISSAOUNITARIO.AsFloat;
   end;
end;

