      '   CLI.CEP,'
      '   CLI.CNPJ,'
      '   CLI.INSC_ESTADUAL,'
      '   CID.DESC_CIDADE,'
      '   EST.SIGLA'
      'from CLIENTE CLI'
      '   left join CIDADE CID on CLI.COD_CIDADE = CID.COD_CIDADE'
      '   left join ESTADO EST on CID.ID_ESTADO = EST.ID_ESTADO'
      '   where CLI.COD_EMPRESA = :COD_EMPRESA'
      '   and CLI.COD_CLIENTE = :COD_CLIENTE')
