  object Qpesq: TSQLQuery
    SQLConnection = DM.BancoDados
    Params = <
      item
        DataType = ftInteger
        Name = 'COD_EMPRESA'
        ParamType = ptInput
      end
      item
        DataType = ftDate
        Name = 'DATA'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'COD_MOTORISTA'
        ParamType = ptInput
      end>
    SQL.Strings = (
      'select'
      '   count(ID_CARGA) AS QTDE'
      'from CARGA'
      '   where COD_EMPRESA = :COD_EMPRESA'
      '   and DATA = :DATA'
      '   and COD_MOTORISTA = :COD_MOTORISTA')
    Left = 720
    Top = 496
    object QpesqQTDE: TIntegerField
      FieldName = 'QTDE'
      Required = True
    end
  end
