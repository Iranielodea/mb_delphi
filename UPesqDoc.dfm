  object QPesq: TSQLQuery
    SQLConnection = DM.BancoDados
    Params = <
      item
        DataType = ftFixedChar
        Name = 'TIPO_CONTA'
        ParamType = ptInput
      end
      item
        DataType = ftString
        Name = 'DOCUMENTO'
        ParamType = ptInput
      end>
    SQL.Strings = (
      'select'
      '   CON.DOCUMENTO,'
      '   CON.DATA_EMISSAO,'
      '   CON.DATA_VENCTO,'
      '   CON.ID_CONTA,'
      '   CON.COD_CLIENTE,'
      '   CON.COD_FOR,'
      '   CLI.NOME,'
      '   FORN.NOME AS NOME_FOR'
      'from CONTAS CON'
      '   left join CLIENTE CLI on CON.COD_CLIENTE = CLI.COD_CLIENTE'
      '   left join FORNECEDOR FORN on CON.COD_FOR = FORN.COD_FOR'
      '   where CON.TIPO_CONTA = :TIPO_CONTA'
      '   and CON.DOCUMENTO like :DOCUMENTO'
      '   order by CON.DATA_EMISSAO')
